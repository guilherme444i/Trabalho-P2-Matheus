fila_de_atendimento = []

fila_de_atendimento.append("Gabriel Nobre")
fila_de_atendimento.append("Gabriel pereira")
fila_de_atendimento.append("Cristian Roveré")
fila_de_atendimento.append("Daniel holanda")

print("Fila atual:", fila_de_atendimento)

cliente_atual = fila_de_atendimento.pop(0)
print(f"\nAtendendo o cliente: {cliente_atual}")

print("\nFila atualizada:", fila_de_atendimento)

fila_de_atendimento.append("Renan")
fila_de_atendimento.append("João")

print("\nFila atualizada com novos clientes:", fila_de_atendimento)

cliente_atual = fila_de_atendimento.pop(0)
print(f"\nAtendendo o cliente: {cliente_atual}")

print("\nFila atualizada após novo atendimento:", fila_de_atendimento)

# Simulação de uma fila de atendimento
fila_de_atendimento = []  # Lista vazia para armazenar os clientes

# Adicionar clientes à fila
fila_de_atendimento.append("Cliente 1")
fila_de_atendimento.append("Cliente 2")
fila_de_atendimento.append("Cliente 3")
fila_de_atendimento.append("Cliente 4")

print("Fila atual:", fila_de_atendimento)

# Atender clientes (remover cliente do início da fila)
cliente_atual = fila_de_atendimento.pop(0)
print(f"Atendendo o cliente: {cliente_atual}")

# Verificar fila após atendimento
print("Fila atualizada:", fila_de_atendimento)

# Adicionar mais clientes à fila
fila_de_atendimento.append("Novo Cliente")
fila_de_atendimento.append("Outro Cliente")

print("Fila atualizada com novos clientes:", fila_de_atendimento)

# Atender mais clientes
cliente_atual = fila_de_atendimento.pop(0)
print(f"Atendendo o cliente: {cliente_atual}")

print("Fila atualizada após novo atendimento:", fila_de_atendimento)
